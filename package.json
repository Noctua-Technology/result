{
  "name": "@noctuatech/result",
  "version": "1.1.0",
  "type": "module",
  "main": "target/lib.js",
  "exports": {
    ".": "./target/lib.js"
  },
  "sideEffects": false,
  "scripts": {
    "build": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**",
        "tsconfig.json"
      ],
      "output": [
        "target/**",
        "tsconfig.tsbuildinfo"
      ]
    },
    "test": {
      "command": "node --test \"./target/**/*.test.js\"",
      "dependencies": [
        "build"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "typescript": "^5.9.3",
    "wireit": "^0.14.12"
  }
}
